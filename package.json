{
  "name": "admyn",
  "version": "1.1.7",
  "description": "An admin panel that integrates into existing Node + Express + MySQL/MariaDB applications.",
  "files": [
    "client/",
    "server/"
  ],
  "scripts": {
    "js": "webpack",
    "css": "node-sass --output-style compressed client/styles/complete.scss client/styles/admyn.css",
    "test": "node server/test",
    "prepublishOnly": "npm run js && npm run css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Xyfir/admyn.git"
  },
  "keywords": [
    "express",
    "mysql",
    "admin",
    "panel",
    "database"
  ],
  "author": "Xyfir, LLC <contact@xyfir.com> (https://xyfir.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Xyfir/admyn/issues"
  },
  "homepage": "https://github.com/Xyfir/admyn#readme",
  "dependencies": {
    "express": "^4.16.1",
    "mysql": "^2.14.1",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-md": "^1.1.5",
    "superagent": "^3.6.1"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "env",
            "react"
          ],
          "extensions": [
            ".jsx",
            ".js"
          ]
        }
      ]
    ]
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "node-sass": "^4.5.3",
    "webpack": "^3.6.0"
  }
}
