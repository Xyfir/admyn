{
  "name": "admyn",
  "version": "1.3.0",
  "description":
    "An admin panel that integrates into existing Node + Express + MySQL/MariaDB applications.",
  "files": ["client/", "server/"],
  "scripts": {
    "build:js": "webpack-cli",
    "build:css":
      "node-sass --output-style compressed client/styles/complete.scss client/styles/admyn.css",
    "test": "node server/test",
    "prepublishOnly": "npm run build:js && npm run build:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Xyfir/admyn.git"
  },
  "keywords": ["express", "mysql", "admin", "panel", "database", "management"],
  "author": "Xyfir, LLC <contact@xyfir.com> (https://xyfir.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Xyfir/admyn/issues"
  },
  "homepage": "https://github.com/Xyfir/admyn#readme",
  "prettier": {
    "singleQuote": true
  },
  "dependencies": {
    "express": "^4.16.3",
    "mysql": "^2.15.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "node-sass": "^4.8.3",
    "prettier": "^1.12.0",
    "prop-types": "^15.6.1",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-md": "^1.2.13",
    "superagent": "^3.8.2",
    "webpack": "^4.5.0"
  },
  "peerDependencies": {
    "react": ">= 16.0.0",
    "react-dom": ">= 16.0.0"
  }
}
